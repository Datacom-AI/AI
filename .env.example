PORT=5000
MONGODB_URI=mongodb://localhost:27017/cpg_crawler
NODE_ENV=development
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_openai_api_key_here
AI_PROVIDER=gemini

# AWS Claude Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_CLAUDE_MODEL=anthropic.claude-3-sonnet-20240229-v1:0
USE_AWS_CLAUDE=false 

# Crawler Configuration
USE_PROXY=false
PROXY_URL=http://proxy-server.crawlera.com:8010
PROXY_USERNAME=your_proxy_username
PROXY_PASSWORD=your_proxy_password
SAVE_SCREENSHOTS=true
SCREENSHOT_PATH=logs/screenshots
NAVIGATION_TIMEOUT=180000
MAX_RETRIES=7
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/121.0.0.0 Safari/537.36 

# Queue Configuration
MAX_CONCURRENT_TASKS=1
QUEUE_RETRY_DELAY=10000 